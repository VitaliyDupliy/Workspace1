package lesson34.lambdas2;

import java.util.List;

import javax.lang.model.element.Element;

public class Test2 {

	public static void main(String[] args) {
		List<String> list = List.of("privet", "poka", "kak dela", "vse norm");
		
		list.forEach(s -> {
			if(!s.startsWith("p")) {
				s=s.toUpperCase();
				System.out.println(s);}
		});
	}
}
